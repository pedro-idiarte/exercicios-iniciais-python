num1 = 0
num2 = 0
num3 = 0
resultado = 0

print("Programa para multiplcar 3 números")
num1 = int(input("Insira o primeiro número: "))
num2 = int(input("Insira o primeiro segundo: "))
num3 = int(input("Insira o primeiro terceiro: "))

resultado = num1 * num2 * num3

print("O resultado da multiplicação é: ", resultado)